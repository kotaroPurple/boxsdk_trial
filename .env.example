# BOX SDK JWT 用サンプル設定 (値はダミーです)
BOX_CLIENT_ID=your_client_id
BOX_CLIENT_SECRET=your_client_secret
BOX_ENTERPRISE_ID=your_enterprise_id
BOX_APP_USER_ID=   # アプリユーザーで認証する場合はIDを入れる、使わない場合は空
BOX_JWT_KEY_ID=your_public_key_id
BOX_JWT_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\\nREPLACE_ME\\n-----END PRIVATE KEY-----
BOX_JWT_PASSPHRASE=your_private_key_passphrase

# Developer Token モード（Enterprise でない個人利用向け）
# これをセットすると JWT 設定は不要になります
BOX_DEVELOPER_TOKEN=

BOX_UPLOAD_FOLDER_ID=123456789   # アップロード先フォルダID

LOCAL_DATA_DIR=./data
UPLOAD_LOG_PATH=.upload_log.json
BOX_MAX_RETRIES=3
